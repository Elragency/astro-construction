  <section class="relative md:-mt-[100px] overflow-y-hidden">
    <div class="flex items-center justify-center h-screen">
      <!-- Image section -->
      <div class="absolute inset-0 z-0">
        <img
          class="w-full h-full object-cover"
          loading="eager"
          src="https://raw.githubusercontent.com/Elragency/astro-construction/8b0df05e9c3187c1bd9e436446b3a2385fb53222/src/assets/images/banners/banner1-img1.webp"
          alt="L'équipe de K-Constructions"
